AnovaMod <- aov(CellCount ~ Marcrophage, data=da2)
summary(AnovaMod)
with(da2, numSummary(CellCount,
groups=Marcrophage, statistics=c("mean", "sd")))
local({
.Pairs <- glht(AnovaMod, linfct =
mcp(Marcrophage = "Tukey"))
print(summary(.Pairs)) # pairwise tests
print(confint(.Pairs, level=0.95))
# confidence intervals
print(cld(.Pairs, level=0.05))
# compact letter display
old.oma <- par(oma=c(0, 5, 0, 0))
plot(confint(.Pairs))
par(old.oma)
})
